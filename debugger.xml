<application>
  <component name="XDebuggerSettings">
    <data-views />
    <general>
      <scroll-to-center>true</scroll-to-center>
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration doNotStepIntoLibraryCode="false">
          <custom-object-presentation enabled="true" />
          <stepping-filters-enabled>false</stepping-filters-enabled>
        </configuration>
      </debugger>
      <debugger id="python">
        <configuration>
          <option name="librariesFilterEnabled" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>